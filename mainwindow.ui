       <string notr="true">/* 全局基础样式 */
QWidget {
    font-family: &quot;PingFang SC Regular&quot;;  /* 成功加载的字体 */
    font-size: 30px;                    /* 基础字号（原 30px 过大，调整为更协调的 14px） */
    color: #333333;
    background-color: #f5f5f5;          /* 界面整体背景 */
}

/* ============== 输入框样式 ============== */
QLineEdit {
    height: 35px;                       /* 与按钮高度一致 */
    border: 1px solid #d1d5db;          /* 浅灰边框 */
    border-radius: 4px;                 /* 圆角 */
    padding: 0 8px;                     /* 内边距 */
    background-color: #ffffff;          /* 白色背景 */
    font-size: 30px;                    /* 文字大小与全局一致 */
}

QLineEdit:focus {
    border-color: #0078d4;              /* 聚焦时边框变主色 */
    outline: none;                      /* 移除默认焦点框 */
}

QLineEdit::placeholder {
    color: #999999;                     /* 占位符灰色 */
}

/* ============== QTabWidget 选项卡样式 ============== */
QTabWidget::tab-bar {
    alignment: left;  /* 选项卡居左排列 */
}

QTabWidget::tab {
    background-color: #e0e0e0;         /* 默认背景 */
    border: 1px solid #cccccc;          /* 边框 */
    border-bottom: none;                /* 底部无边框（与内容区衔接） */
    padding: 8px 16px;                  /* 内边距（上下 8px，左右 16px） */
    margin-right: 4px;                  /* 右侧间距 */
    border-top-left-radius: 4px;        /* 左上圆角 */
    border-top-right-radius: 4px;       /* 右上圆角 */
    font-size: 14px;                    /* 文字大小与全局一致 */
}

QTabWidget::tab:selected {
    background-color: #ffffff;          /* 选中时背景变白 */
    border-color: #cccccc;              /* 边框颜色不变 */
    border-bottom-color: #ffffff;       /* 底部边框颜色与内容区背景一致（覆盖） */
}

QTabWidget::tab:hover {
    background-color: #f0f0f0;          /* 悬停时背景变亮 */
}

/* ============== 按钮样式 ============== */
QPushButton {
    min-width: 80px;                    /* 最小宽度（避免过窄） */
    height: 35px;                       /* 统一高度（与输入框一致） */
    border: none;                       /* 无边框 */
    border-radius: 4px;                 /* 圆角 */
    padding: 0 12px;                    /* 内边距 */
    color: #ffffff;                     /* 文字白色 */
    background-color: #0078d4;          /* 主色（蓝色） */
    /* 移除 Qt 不支持的 transition 属性 */
}

QPushButton:hover {
    background-color: #005a9c;          /* 悬停加深 */
    cursor: pointer;                    /* 手型光标（Qt 支持此属性，需确保拼写正确） */
}

QPushButton:pressed {
    background-color: #004578;          /* 按下更深 */
}

QPushButton:disabled {
    background-color: #e5e7eb;          /* 禁用浅灰 */
    color: #6b7280;                     /* 禁用灰文字 */
}

/* 特殊功能按钮（如“红外测温仪通信”） */
QPushButton#openDeviceCommunicationButton {
    background-color: #10b981;          /* 绿色（区别主色） */
    height: 35px;                       /* 与普通按钮高度一致 */
    font-size: 30px;                    /* 文字大小与全局一致（原 30px 过大） */
}

QPushButton#openDeviceCommunicationButton:hover {
    background-color: #059669;          /* 绿色加深 */
}

/* ============== QGroupBox 分组框样式 ============== */
QGroupBox {
    background-color: #f8f9fa;          /* 浅灰背景 */
    border: 1px solid #e5e7eb;          /* 浅灰边框 */
    border-radius: 8px;                 /* 圆角 */
    margin-top: 24px;                   /* 顶部边距 */
    padding: 16px;                      /* 内边距 */
    font-size: 16px;                    /* 标题字号（比基础字号大 2px） */
    font-weight: bold;                  /* 标题加粗 */
    color: #111827;                     /* 标题深灰 */
}

QGroupBox::title {
    subcontrol-origin: margin;          /* 标题位置基于 margin */
    subcontrol-position: top left;      /* 标题居左 */
    left: 8px;                          /* 左偏移 */
    padding: 0 4px;                     /* 内边距 */
    background-color: #f5f5f5;          /* 标题背景与主窗口一致 */
}

/* ============== 进度条样式 ============== */
QProgressBar {
    height: 30px;                       /* 高度 */
    border: 1px solid #e5e7eb;          /* 浅灰边框 */
    border-radius: 6px;                 /* 圆角 */
    background-color: #f3f4f6;          /* 背景色 */
    text-align: center;                 /* 文字居中 */
    font-size: 12px;                    /* 文字大小 */
}

QProgressBar::chunk {
    background-color: #0078d4;          /* 进度条颜色与按钮主色一致 */
    border-radius: 5px;                 /* 圆角略小于外层 */
}

